rules:
  "no-todo": true
  write-good: false
  retext: false
  unexpanded-acronym: 
    ignore_acronyms: ["CUDA", "GPU"]
  stop-words:
    defaultWords: true
    words: ["obviously", "basically", "simply", "of course", "clearly", "just", "everyone knows", "however", "so", "easy"]
  terminology: true
  apostrophe: true
  alex: true
  "no-dead-link": true
